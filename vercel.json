{
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/v1/api/hello/:path*",
            "destination": "/api/hello.go"
        },
        {
            "source": "/v1/api/:path*",
            "destination": "/api/:path*"
        }
    ],
    "redirects": [
        {
            "source": "/api/:path*",
            "destination": "/v1/api/:path*"
        }
    ],
    "build": {
        "env": {
            "GO_BUILD_FLAGS": "-ldflags '-s -w'"
        }
    }
}
